{
	"easycom": {
		// 注意一定要放在custom里，否则无效，https://ask.dcloud.net.cn/question/131175
		"custom": {
			"^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
			"^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
	        "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue"
		}
	},
	"pages": [{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页"
			}
		},
		{
			"path": "pages/reservation/reservation",
			"style": {
				"navigationBarTitleText": "预约申请"
			}
		},
		{
			"path": "pages/web_danye/web_danye",
			"style": {
				"navigationBarTitleText": ""
			}
		}, {
			"path": "pages/cateList/cateList",
			"style": {
				"navigationBarTitleText": "分类",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/product/productList",
			"style": {
				"navigationBarTitleText": "商品列表",
				"enablePullDownRefresh": false,
				"onReachBottomDistance": 80
			}

		}, {
			"path": "pages/product/productDetail",
			"style": {
				"navigationBarTitleText": "商品详情",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/home/home",
			"style": {
				"navigationBarTitleText": "首页",
				"enablePullDownRefresh": false
			}

		},
		{
			"path": "pages/reservation_list/reservation_list",
			"style": {
				"navigationBarTitleText": "预约列表",
				"onReachBottomDistance": 80
			}
		}
	],
	"subPackages": [{
		"root": "pages_user",
		"pages": [{
				"path": "index/index",
				"style": {
					"navigationBarTitleText": "个人中心"
				}
			},
			{
				"path": "info/info",
				"style": {
					"navigationBarTitleText": "我的信息"
				}
			},
			{
				"path": "reservation_list/reservation_list",
				"style": {
					"navigationBarTitleText": "我的预约",
					"onReachBottomDistance": 80
				}
			}, {
				"path": "address/addressList",
				"style": {
					"navigationBarTitleText": "地址管理",
					"enablePullDownRefresh": false
				}

			}, {
				"path": "address/addressDetail",
				"style": {
					"navigationBarTitleText": "地址详情",
					"enablePullDownRefresh": false
				}

			}
		]
	}],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#fff",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {}
}